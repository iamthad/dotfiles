set nocompatible
filetype on
filetype plugin on
filetype plugin indent on
let fortran_free_source=1
syntax enable
set expandtab
set smarttab
set autoindent
set virtualedit=onemore
set shiftwidth=2
set softtabstop=2
set number
set ignorecase
set smartcase
set cursorline
colorscheme Tomorrow-Night-Bright
